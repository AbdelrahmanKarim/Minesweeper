<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.Settinggs">

    <TextView
        android:id="@+id/aaaa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Settings"
        android:textSize="40sp"
        android:fontFamily="@font/jakartasans"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="2sp"
        android:layout_marginStart="15sp"
        android:textColor="#000"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:onClick="noads"

        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/roundbtn"
        android:layout_marginStart="15sp"
        android:layout_marginEnd="15sp"
        android:id="@+id/bbbb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12sp"
        android:fontFamily="@font/jakartasans"
        android:text="NO ADS"
        android:textColor="#000"
        android:textSize="20sp"
        android:layout_marginRight="60sp"
        app:layout_constraintTop_toBottomOf="@+id/aaaa" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/cccc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_marginStart="15sp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="15sp"
        android:background="@drawable/woundbtn"
        android:fontFamily="@font/jakartasans"
        android:onClick="htpl"
        android:text="How to play"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bbbb" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/dddd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_marginStart="15sp"
        android:layout_marginTop="20sp"
        android:layout_marginEnd="15sp"
        android:background="@drawable/woundbtn"
        android:fontFamily="@font/jakartasans"
        android:onClick="stat"
        android:text="Statistics"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cccc" />

    <ImageView
        android:id="@+id/mnn"
        android:layout_width="match_parent"
        android:layout_height="250sp"
        android:layout_marginTop="64dp"
        android:src="@drawable/fbmb"
        app:layout_constraintTop_toBottomOf="@+id/dddd"
        tools:layout_editor_absoluteX="0dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/nggg"
        android:layout_width="match_parent"

        android:layout_height="wrap_content"
        android:layout_marginStart="15sp"
        android:layout_marginTop="100sp"
        android:layout_marginEnd="15sp"
        android:background="@drawable/woundbtn"
        android:fontFamily="@font/jakartasans"
        android:text="Sign up"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mnn" />

